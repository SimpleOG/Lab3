#include <iostream>
#include <clocale>
#include <cmath>
using namespace std;

int main()

{setlocale(LC_ALL, "RUS");

    double a,c; 
    int N;
    cout << "Введите число" << endl;
    cin>>a;
    c=1;
    cout<<"Введите степень числа"<<endl;
    cin>>N;
    for(int i=0; i<N;i++){
        c=c*a;
        cout<<c<<" ";

    }
    return 0;


}
